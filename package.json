{
	"name": "next-template-app",
	"version": "1.0.0",
	"private": true,
	"description": "Next Template APP",
	"homepage": "https://github.com/tisoap/next-template-app#readme",
	"repository": {
		"type": "git",
		"url": "git@github.com:tisoap/next-template-app.git"
	},
	"license": "MIT",
	"author": "Tiso Alvarez Puccinelli",
	"scripts": {
		"build": "yarn build-storybook && next build",
		"build-storybook": "build-storybook --quiet --loglevel error",
		"check": "yarn type-check && yarn spellcheck && yarn lint && yarn test",
		"clean": "rm -rf ./storybook-static && rm -rf ./node_modules && rm -rf ./.next",
		"coverage": "yarn test && http-server coverage/lcov-report",
		"dev": "dotenv -e .local.env next dev",
		"generate-theme-typings": "chakra-cli tokens --strict-component-types ./providers/Theme/theme.ts",
		"lint": "eslint --report-unused-disable-directives --fix ./ --ext .js,.ts,.tsx",
		"spellcheck": "cspell --no-progress '**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}'",
		"start": "next start",
		"storybook": "start-storybook -p 6006 --quiet",
		"test": "jest --config jest.config.ts --no-cache",
		"test-e2e": "dotenv -e .local.env testcafe firefox e2e/tests",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@chakra-ui/react": "1.8.3",
		"@chakra-ui/system": "1.10.3",
		"@chakra-ui/theme": "1.13.2",
		"@chakra-ui/theme-tools": "1.3.4",
		"@chakra-ui/utils": "1.10.2",
		"@emotion/react": "11.7.1",
		"@emotion/styled": "11.6.0",
		"@reduxjs/toolkit": "1.7.2",
		"axios": "0.25.0",
		"framer-motion": "6.2.4",
		"lodash": "4.17.21",
		"next": "12.0.10",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-redux": "7.2.6",
		"redux": "4.1.2",
		"reselect": "4.1.5"
	},
	"devDependencies": {
		"@babel/core": "7.17.0",
		"@babel/preset-typescript": "7.16.7",
		"@chakra-ui/cli": "1.8.1",
		"@chakra-ui/storybook-addon": "1.0.2",
		"@storybook/addon-actions": "6.4.18",
		"@storybook/addon-essentials": "6.4.18",
		"@storybook/addon-links": "6.4.18",
		"@storybook/addons": "6.4.18",
		"@storybook/builder-webpack5": "6.4.18",
		"@storybook/manager-webpack5": "6.4.18",
