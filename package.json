{
	"name": "next-template-app",
	"version": "1.0.0",
	"private": true,
	"description": "Next Template APP",
	"homepage": "https://github.com/tisoap/next-template-app#readme",
	"repository": {
		"type": "git",
		"url": "git@github.com:tisoap/next-template-app.git"
	},
	"license": "MIT",
	"author": "Tiso Alvarez Puccinelli",
	"scripts": {
		"build": "yarn build-storybook && next build",
		"build-storybook": "build-storybook --quiet --loglevel error",
		"check": "yarn type-check && yarn spellcheck && yarn lint && yarn test",
		"clean": "rm -rf ./storybook-static && rm -rf ./node_modules && rm -rf ./.next",
		"coverage": "yarn test && http-server coverage/lcov-report",
		"dev": "dotenv -e .local.env next dev",
		"generate-theme-typings": "chakra-cli tokens --strict-component-types ./providers/Theme/theme.ts",
		"lint": "eslint --report-unused-disable-directives --fix ./ --ext .js,.ts,.tsx",
		"spellcheck": "cspell --no-progress '**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}'",
		"start": "next start",
		"storybook": "start-storybook -p 6006 --quiet",
		"test": "jest --config jest.config.ts",
		"test-e2e": "dotenv -e .local.env testcafe firefox:headless e2e/tests",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@chakra-ui/react": "1.8.8",
		"@chakra-ui/system": "1.12.1",
		"@chakra-ui/theme": "1.14.1",
		"@chakra-ui/theme-tools": "1.3.6",
		"@chakra-ui/utils": "1.10.4",
		"@emotion/react": "11.9.0",
		"@emotion/styled": "11.8.1",
		"@reduxjs/toolkit": "1.8.1",
		"@tisoap/react-flow-smart-edge": "1.0.1",
		"axios": "0.27.2",
		"framer-motion": "6.3.3",
		"lodash": "4.17.21",
		"next": "12.1.6",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-flow-renderer": "10.2.2",
		"react-redux": "8.0.1",
		"redux": "4.2.0",
		"redux-persist": "6.0.0",
		"reselect": "4.1.5"
	},
	"devDependencies": {
		"@babel/core": "7.17.10",
		"@babel/preset-typescript": "7.16.7",
		"@chakra-ui/cli": "1.9.0",
